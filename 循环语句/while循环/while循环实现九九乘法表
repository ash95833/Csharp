 //while循环嵌套打印九九乘法表
            int i = 1;
            while (i <= 9)
            {
                int j = 1;
                while (j <= i)
                {
                    Console.Write("{0}*{1}={2}\t", j, i, i * j);

                    j++;
                }
                i++;
                Console.WriteLine("\n");

            }
            Console.WriteLine();
            Console.ReadKey();
