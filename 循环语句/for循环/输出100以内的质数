// 输出100以内的质数
            for (int i = 2; i <= 100; i++)
            {
                bool b = true;
                for (int j = 2; j < i ; j++)
                {
                    if (i % j ==0)
                    {
                        b = false;
                        break;
                    }
                }
                if (b)
                {
                    Console.WriteLine(i);
                }
            }
